{
  "name": "next-starter",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "schema": "src/db/prisma/schema.prisma"
  },
  "scripts": {
    "dev": "TAILWIND_MODE=watch concurrently -r \"npx next\" \"yarn watch:graphql-codegen\"",
    "build": "next build",
    "start": "next start",
    "migrate:dev": "prisma migrate dev",
    "generate": "yarn generate:prisma && yarn generate:nexus && yarn generate:graphql-codegen",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json src/server/graphql/schema.ts",
    "generate:graphql-codegen": "graphql-codegen",
    "watch:graphql-codegen": "graphql-codegen --watch",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint --ext js,jsx,ts,tsx"
  },
  "dependencies": {
    "@nexus/schema": "^0.20.1",
    "@prisma/client": "2.16.1",
    "@tailwindcss/jit": "^0.1.3",
    "@tailwindui/react": "^0.1.1",
    "@types/classnames": "^2.2.11",
    "apollo-errors": "^1.9.0",
    "apollo-resolvers": "^1.4.2",
    "apollo-server": "^2.20.0",
    "apollo-server-micro": "^2.20.0",
    "autoprefixer": "^10.2.4",
    "bcrypt": "^5.0.0",
    "classnames": "^2.2.6",
    "concurrently": "^6.0.0",
    "graphql": "^15.5.0",
    "klona": "^2.0.4",
    "next": "10.0.6",
    "next-auth": "^3.4.1",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.30.0",
    "nexus-shield": "^2.0.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "postcss": "^8.2.8",
    "rc-trigger": "^5.2.3",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-icons": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "tailwindcss": "^2.0.4",
    "ts-node": "^9.1.1",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.31",
    "urql": "^2.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.3",
    "@graphql-codegen/typescript": "1.21.1",
    "@graphql-codegen/typescript-operations": "1.17.15",
    "@graphql-codegen/typescript-react-apollo": "2.2.3",
    "@graphql-codegen/typescript-urql": "^2.0.5",
    "@types/next-auth": "^3.7.2",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "prisma": "2.16.1",
    "typescript": "^4.1.5"
  }
}
